<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TargetPrefix">

    <Button
        android:id="@+id/button_kiB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kiB"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_MiB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MiB"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_kiB" />

    <Button
        android:id="@+id/button_GiB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GiB"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_MiB" />

    <Button
        android:id="@+id/button_TiB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TiB"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_GiB" />

</androidx.constraintlayout.widget.ConstraintLayout>